	static void NullButton() {
		for(int i=0;i<9;i++)
			buttonI[i]=2;
	}
	
  static void checkColour() {
	  X= textFieldPlayer1Score.getText();
  	  O= textFieldPlayer2Score.getText();
  	if(Integer.parseInt(X)>Integer.parseInt(O)) {
  		textFieldPlayer2Score.setForeground(Color.RED);
  		textFieldPlayer1Score.setForeground(Color.BLACK);
  		textFieldPlayer1Score.setBackground(new Color(240, 255, 240));
  		textFieldPlayer2Score.setBackground(new Color(240, 255, 240));
  	}
  	else 	if(Integer.parseInt(X)<Integer.parseInt(O)) {
  		textFieldPlayer1Score.setForeground(Color.RED);
  		textFieldPlayer2Score.setForeground(Color.BLACK);
  		textFieldPlayer1Score.setBackground(new Color(240, 255, 240));
  		textFieldPlayer2Score.setBackground(new Color(240, 255, 240));
  	}
  	else 	if(Integer.parseInt(X)==Integer.parseInt(O)) {
  		textFieldPlayer1Score.setBackground(new Color(240, 255, 240));
  		textFieldPlayer2Score.setBackground(new Color(240, 255, 240));
  		textFieldPlayer1Score.setForeground(Color.BLACK);
  		textFieldPlayer2Score.setForeground(Color.BLACK);
  	}
  	
  }
   
